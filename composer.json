{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^7.2",
        "guzzlehttp/promises": "^1.4",
        "guzzlehttp/psr7": "^1.7",
        "htmlmin/htmlmin": "^8.0",
        "jeroennoten/laravel-adminlte": "^3.4",
        "kalnoy/nestedset": "^5.0",
        "kyslik/column-sortable": "^6.3",
        "laravel/cashier": "^12.4",
        "laravel/framework": "^7.0",
        "laravel/horizon": "4.x-dev",
        "laravel/slack-notification-channel": "^2.3",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.1",
        "league/csv": "^9.6",
        "league/flysystem-aws-s3-v3": "1.0.x-dev",
        "predis/predis": "^1.1",
        "spatie/laravel-backup": "^6.14",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Models\\": "app/Models/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/lib/storeItemSet.php",
            "app/lib/keywordItemSet.php",
            "app/lib/stocksSet.php",
            "app/lib/pricesSet.php",
            "app/lib/smCateItemSet.php",
            "app/lib/keyCateItemSet.php",
            "app/lib/itemSet.php",
            "app/lib/distanceSet.php",
            "app/lib/csvErrorList.php",
            "app/lib/productCount.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan horizon:publish --ansi"
        ]
    }
}
